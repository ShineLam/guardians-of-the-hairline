
.preview {
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  color: #fff;
  padding: 0 50rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 2000;
}

.preview-img-wrap {
  width: 100%;
  height: 438rpx;
  border-radius: 8rpx;
  background: #ededed;
  overflow: hidden;
}

.preview-img-wrap .img {
  width: 100%;
  height: 100%;
}

.preview > .vw {
  position: relative;
  z-index: 650;
}
